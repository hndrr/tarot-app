{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "node": "20.11.0",
      "pnpm": "9.1.0",
      "prebuildCommand": "npx expo prebuild --platform android --clean"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "node": "20.11.0",
      "pnpm": "9.1.0",
      "prebuildCommand": "npx expo prebuild --platform android --clean"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "node": "20.11.0",
      "pnpm": "9.1.0",
      "prebuildCommand": "npx expo prebuild --platform android --clean"
    },
    "release": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
